#include <iostream>
#include <string>
#include<cmath>
#include <iomanip>
using namespace std;

int main() 
{
    double a = 2.3;
    double b = 2.3;
    double c = 2.123456;
    double d = 2.123456;
    double e = 2.123456;

    cout << a << endl;
    cout << fixed << b << endl;
    cout << fixed << setprecision(6) << c << endl;
    cout << fixed << setprecision(2) << d << endl;
    cout << scientific << e << endl;

}
