#include <iostream>
using namespace std;

int main() 
{
    int a, b, c, d;

    cout << "Enter first number: ";
    cin >> a;
    cout << "Enter second number: ";
    cin >> b;
    cout << "Enter third number: ";
    cin >> c;
    cout << "Enter fourth number: ";
    cin >> d;

    if (a >= 1 && a <= 255 &&
        b >= 1 && b <= 255 &&
        c >= 1 && c <= 255 &&
        d >= 1 && d <= 255) 
{

        cout << a << "." << b << "." << c << "." << d << endl;
    } else {
        cout << "Invalid IP number" << endl;
    }

}
