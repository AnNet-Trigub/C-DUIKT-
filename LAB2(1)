#include <iostream>
#include <cmath>

int main() {
    double x, y;

    std::cout << "Enter x: ";
    std::cin >> x;

    std::cout << "Enter y: ";
    std::cin >> y;

    double result;

    if (x <= 0) {
        result = x * x - sin(y);
    } else {
        result = sqrt(x) + cos(y);
    }

    std::cout << "Result = " << result << std::endl;

    return 0;
}
